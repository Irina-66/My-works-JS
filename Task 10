Даны числа a и b. Выведите строку с числами от большего числа до меньшего. Известно, что и a и b больше либо равно 1, но неизвестно, какое из них больше.
function testCycle(a, b) {
    var x=" ";
    if(a<b){
        for(i=b;i>=a;i--){
            x+=" "+i;
        }
    }else if(b<a){
        for(i=a;i>=b;i--){
            x+=" "+i;
        }
    }else if(a==b){
        x=a;
    }
        return x;
}
