Даны числа a и b. Выведите строку с числами между а и b включая границы, отсортированными по возрастанию. Неизвестно, какое из чисел больше, но известно, что и a и b больше 0.
function testCycle(a, b) {
    var x=" ";
    for(i=a;i<=b;i++){
        x+=i+" ";
    }
    for(y=b;y<=a;y++){
        x+=y+" ";
    }
    if(a==b){
        x=a;
        
    }
    return x;
}
