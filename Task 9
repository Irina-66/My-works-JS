Даны числа a и b. Найдите сумму квадратов чисел между a и b включительно. Неизвестно, какое из чисел a или b больше.
function testCycle(a, b) {
    var x=0;
    if(a>b){
        for(i=b;i<=a;i++){
            x+=i*i;}
    }else if(b>=a){
        for(i=a;i<=b;i++){
        x+=i*i;
    }
}else if(a=b){
    x=a*a;
}
     return x;
}
